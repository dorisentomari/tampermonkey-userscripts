  "description": "油猴脚本构建，可以构建多个脚本",
  "scripts": {
    "prettier": "prettier --write .",
    "zadp:dev": "node ./build/runCmd.js --block-code zadp --mode development",
    "zadp:build": "node ./build/runCmd.js --block-code zadp --mode production",
    "tank.utils:dev": "node ./build/runCmd.js --block-code tank.utils --mode development",
    "tank.utils:build": "node ./build/runCmd.js --block-code tank.utils --mode production",
    "third.lib:lodash:dev": "node ./build/runCmd.js --block-code third.lib --mode development --lib lodash",
    "third.lib:lodash:build": "node ./build/runCmd.js --block-code third.lib --mode production --lib lodash",
    "third.lib:jquery:dev": "node ./build/runCmd.js --block-code third.lib --mode development --lib jquery",
    "third.lib:jquery:build": "node ./build/runCmd.js --block-code third.lib --mode production --lib jquery"
  },
